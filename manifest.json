{
  "manifest_version": 2,

  "name": "Skills Matrix",
  "description": "This extension helps you with job searching.",
  "version": "1.0",
  "options_page": "index.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Skills Matrix",
    
    "default_popup": "index.html"
  
  },

    "permissions": [
    "activeTab",
    "tabs"
  ]
}
